    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "serverless": "^1.29.2",
    "serverless-offline": "^3.25.0",
    "sinon": "^5.0.10",
    "sinon-chai": "^3.1.0"
  },
  "dependencies": {
    "aws-sdk": "^2.271.1",
